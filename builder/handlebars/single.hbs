<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
  <meta charset="utf-8">
  <title>{{options.title}}</title>

  <meta name="description" content="">
  <meta name="generator" content="kss-node">
  <meta name="viewport" content="width=device-width">

  <link rel="stylesheet" href="kss-assets/kss.css">
  {{{styles}}}
</head>
<body id="kss-node--single">

<nav class="kss-top-nav">
  <ul class="kss-top-nav__menu">
    <li class="kss-top-nav__menu-item">
      <a class="kss-top-nav__menu-link" href="./">
        <span class="kss-top-nav__ref">0</span
        ><span class="kss-top-nav__name">Overview</span>
      </a>
    </li>
    {{#each menu}}
      <li class="kss-top-nav__menu-item">
        <a class="kss-top-nav__menu-link" href="section-{{referenceURI}}.html">
          <span class="kss-top-nav__ref">{{referenceNumber}}</span><span class="kss-top-nav__name">{{header}}</span>
        </a>
      </li>
    {{/each}}
  </ul>
</nav>

{{!
The sections variable is an array containing a single section.
}}
{{#each sections}}
  <div id="kssref-{{referenceURI}}">

    <div class="kss-style">
      <h1 class="kss-title kss-title--level-1">
        <a class="kss-title__permalink" href="#kssref-{{referenceURI}}">
          <span class="kss-title__ref">
            {{referenceNumber}}
            <span class="kss-title__permalink-hash">
              {{#if @root.hasNumericReferences}}
                #permalink
              {{else}}
                #{{reference}}
              {{/if}}
            </span>
          </span>
          {{header}}
        </a>
      </h1>

      {{#if description}}
        <div class="kss-description">
          {{{description}}}
        </div>
      {{/if}}

      {{#if parameters}}
        <div class="kss-parameters__title">Parameters:</div>
        <ul class="kss-parameters">
          {{#each parameters}}
            <li class="kss-parameters__item">
              <div class="kss-parameters__name"><code>{{name}}</code></div>
              {{#if defaultValue}}
                <div class="kss-parameters__default-value">Defaults to: <code>{{defaultValue}}</code></div>
              {{else}}
                <div class="kss-parameters__no-default"></div>
              {{/if}}
              <div class="kss-parameters__description">{{{description}}}</div>
            </li>
          {{/each}}
        </ul>
        {{#if sourceFile.name}}
          <div class="kss-source">
            Source: <code>{{sourceFile.name}}</code>, line {{sourceFile.line}}
          </div>
        {{/if}}
      {{/if}}
    </div>

    {{#if markup}}
      <div class="kss-modifier__heading kss-style">
        Example{{#if modifiers}}s{{/if}}
      </div>
      {{#if modifiers}}
        <div class="kss-modifier__default-name kss-style">
          Default styling
        </div>
      {{/if}}
      <div class="kss-modifier__example">
        {{{markup}}}
      </div>
      {{#each modifiers}}
        <div class="kss-modifier__name kss-style">
          {{name}}
        </div>
        <div class="kss-modifier__description kss-style">
          {{{description}}}
        </div>
        <div class="kss-modifier__example">
          {{{markup ../this modifier_class=className}}}
        </div>
      {{/each}}

    <div class="kss-markup kss-style">
      <pre class="prettyprint linenums lang-html"><code data-language="html">{{markup}}</code></pre>
    </div>

    {{#if sourceFile.name}}
    <div class="kss-source kss-style">
      Source: <code>{{sourceFile.name}}</code>, line {{sourceFile.line}}
    </div>
    {{/if}}
    {{/if}}

  </div>
{{/each}}

<!-- SCRIPTS -->
<script src="kss-assets/kss.js"></script>
<script src="kss-assets/prettify.js"></script>
<script>
  prettyPrint();
</script>
{{{scripts}}}

<!-- Automatically built using <a href="https://github.com/kss-node/kss-node">kss-node</a>. -->
</body>
</html>
